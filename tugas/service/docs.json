{
  "tags": {
		"allowUnknownTags": true
	},
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./service"],
    "exclude": []
  },
  "opts": {
    "destination": "./docs/service",
    "recurse": true,
    "readme": "service/README.md",
    "encoding": "utf8",
    "template": "./node_modules/docdash"
  },
  "metadata": {
		"title": "Some title"
	},
  "templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	}
}
